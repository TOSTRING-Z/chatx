{
  "memory_length": 10,
  "retry_time": 10,
  "icon_time": 5,
  "prompt": "",
  "info_template": "阶段: {step}, 调用: {model}, 版本: {version}, 输出: \n\n```\n{output_format}\n```\n\n",
  "default": {
    "model": "deepseek",
    "version": "deepseek-chat",
    "plugin": "百度翻译[旧]"
  },
  "llm_parmas": {
    "max_tokens": 4000,
    "temperature": 0.7,
    "stream": true
  },
  "func_status": {
    "clip": true,
    "react": true,
    "markdown": true,
    "math": true,
    "text": false
  },
  "short_cut": "CommandOrControl+Shift+Space",
  "models": {
    "ollama": {
      "api_url": "http://localhost:11434/api/chat",
      "versions": [
        "llama3.2",
        {
          "version": "gemma3:12b",
          "vision": [
            "image"
          ],
          "ollama": true
        }
      ]
    },
    "aliyuncs": {
      "api_url": "https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions",
      "api_key": "",
      "versions": [
        "qwen-plus"
      ]
    },
    "qianfan": {
      "api_url": "https://qianfan.baidubce.com/v2/chat/completions",
      "api_key": "",
      "versions": [
        "deepseek-v3",
        "ernie-speed-128k",
        "ernie-lite-8k",
        "ernie-tiny-8k"
      ]
    },
    "together": {
      "api_key": "",
      "api_url": "https://api.together.xyz/v1/chat/completions",
      "versions": [
        "deepseek-ai/DeepSeek-R1-Distill-Llama-70B-free",
        {
          "version": "meta-llama/Llama-Vision-Free",
          "vision": [
            "image"
          ]
        },
        "meta-llama/Llama-3.3-70B-Instruct-Turbo-Free"
      ]
    },
    "gptgod": {
      "api_url": "https://gptgod.online/api/v1/chat/completions",
      "api_key": "",
      "versions": [
        {
          "version": "gpt-4o-mini",
          "vision": [
            "image"
          ]
        },
        {
          "version": "claude-3-5-sonnet-20241022",
          "vision": [
            "image"
          ]
        },
        "gpt-3.5-turbo",
        "o1-mini-2024-09-12",
        "o1-preview-2024-09-12",
        "gpt-4o-2024-08-06",
        "gpt-4",
        "gpt-4-turbo-preview"
      ]
    },
    "chatglm": {
      "api_url": "https://open.bigmodel.cn/api/paas/v4/chat/completions",
      "api_key": "",
      "versions": [
        "glm-4-flash",
        "glm-zero-preview",
        "glm-4-plus",
        "glm-4-0520",
        "glm-4-alltools",
        "glm-4-long",
        {
          "version": "glm-4v-flash",
          "vision": [
            "image"
          ]
        },
        {
          "version": "glm-4v-plus",
          "vision": [
            "image"
          ]
        }
      ]
    },
    "deepseek": {
      "api_url": "https://api.deepseek.com/chat/completions",
      "api_key": "",
      "versions": [
        "deepseek-chat",
        "deepseek-reasoner"
      ]
    },
    "siliconflow": {
      "api_url": "https://api.siliconflow.cn/v1/chat/completions",
      "api_key": "",
      "versions": [
        "AIDC-AI/Marco-o1",
        "Qwen/Qwen2.5-7B-Instruct",
        "internlm/internlm2_5-7b-chat",
        "meta-llama/Meta-Llama-3.1-8B-Instruct",
        "THUDM/glm-4-9b-chat",
        "deepseek-ai/deepseek-vl2",
        {
          "version": "Pro/Qwen/Qwen2-VL-7B-Instruct",
          "vision": [
            "image"
          ]
        }
      ]
    }
  },
  "plugins": {
    "百度翻译[新]": {
      "path": "{resourcesPath}/resource/plugins/script/trans_baidu_new.js",
      "show": true
    },
    "百度翻译[旧]": {
      "path": "{resourcesPath}/resource/plugins/script/trans_baidu_old.js",
      "show": true
    },
    "谷歌翻译": {
      "path": "{resourcesPath}/resource/plugins/script/trans_google.js",
      "show": true
    },
    "影响因子查询": {
      "path": "{resourcesPath}/resource/plugins/script/if_pmid.js",
      "show": true
    },
    "baidu_search": {
      "path": "{resourcesPath}/resource/plugins/script/baidu_search.js",
      "params": {
        "jina": "",
        "num_results": 5,
        "text_max_len": 1000
      }
    },
    "get_think": {
      "path": "{resourcesPath}/resource/plugins/script/get_think.js"
    },
    "json_parse": {
      "path": "{resourcesPath}/resource/plugins/script/json_parse.js"
    },
    "file_load": {
      "path": "{resourcesPath}/resource/plugins/script/file_load.js",
      "extre": [
        {
          "type": "file-reader"
        }
      ],
      "params": {
        "threshold": 10000
      }
    },
    "search_files": {
      "path": "{resourcesPath}/resource/plugins/script/search_files.js"
    },
    "list_files": {
      "path": "{resourcesPath}/resource/plugins/script/list_files.js",
      "params": {
        "threshold": 50
      }
    },
    "write_to_file": {
      "path": "{resourcesPath}/resource/plugins/script/write_to_file.js"
    },
    "replace_in_file": {
      "path": "{resourcesPath}/resource/plugins/script/replace_in_file.js"
    },
    "python_execute": {
      "path": "{resourcesPath}/resource/plugins/script/python_execute.js",
      "params": {
        "python_bin": "python",
        "threshold": 10000
      }
    },
    "llm_ocr": {
      "path": "{resourcesPath}/resource/plugins/script/llm_ocr.js",
      "params": {
        "version": "claude-3-5-sonnet-20241022",
        "api_url": "",
        "api_key": ""
      }
    }
  },
  "mcp_server": {
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "enabled": true
    }
  },
  "chain_call": [
    {
      "end": true
    }
  ],
  "tool_call": {
    "memory_length": 20,
    "llm_parmas": {
      "max_tokens": 4000,
      "temperature": 0.5,
      "stream": true,
      "response_format": {
        "type": "json_object"
      }
    }
  },
  "extre": [
    {
      "type": "system-prompt"
    }
  ]
}